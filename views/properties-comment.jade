tr(id=symbol.ctx.name)
  td= symbol.ctx.name
  td
    div(class=["label", "label-info", "radius", "ctx-type"])= symbol.type
  td
    if symbol.description && symbol.description.full
      | !{symbol.description.full}
  td
    if symbol.description && symbol.description.extra
      | !{symbol.description.extra}